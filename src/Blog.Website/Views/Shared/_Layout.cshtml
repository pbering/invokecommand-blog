<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title - invokecommand.net</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="content-type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="/rss.xml" type="application/rss+xml" rel="alternate" title="invokecommand.net" />
    <environment names="Development">
        <link rel="stylesheet" href="/css/poole.css">
        <link rel="stylesheet" href="/css/hyde.css">
        <link rel="stylesheet" href="/css/site.css">
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
</head>
<body>
    <div class="sidebar">
        <div class="container sidebar-sticky">
            <div class="sidebar-about">
                <h2>
                    <a href="/">
                        invokecommand.net
                    </a>
                </h2>
                <p class="lead">thoughts about code, devops and technology</p>
            </div>
            <nav class="sidebar-nav">
                <a class="sidebar-nav-item" href="https://twitter.com/pbering">twitter.com/pbering</a>
                <a class="sidebar-nav-item" href="https://github.com/pbering">github.com/pbering</a>
                <a class="sidebar-nav-item" href="/rss.xml">RSS</a>
            </nav>
        </div>
    </div>
    <div class="content container">
        @RenderBody()
    </div>
</body>
</html>